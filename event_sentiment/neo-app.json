{
  "welcomeFile": "/webapp/index.html",
  "routes": [
    {
      "path": "/resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/resources",
        "version": "1.44.13"
      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/test-resources",
        "version": "1.44.13"
      },
      "description": "SAPUI5 Test Resources"
    },
    {
      "path": "/twitter",
      "target": {
        "type": "destination",
        "name": "twitter_demo",
        "entryPath": "/"
      },
      "description": "Twitter REST API"
    }
  ],
  "sendWelcomeFileRedirect": true,
  "cacheControl": [
    {
      "directive": "private",
      "maxAge": 0,
      "path": "**.html"
    },
    {
      "directive": "private",
      "maxAge": 0,
      "path": "sap-ui-cachebuster-info.json"
    },
    {
      "directive": "public",
      "maxAge": 31536000
    }
  ]
}